{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "lib": [
      "dom",
      "es2016"
    ],
    "outDir": "./lib",
  },
  "exclude": [
    "output"
  ]
}
